{
  "name": "movieparadise",
  "version": "1.0.0",
  "repository": "https://gitlab.com/iamdhrooov/movieparadise.git",
  "author": "Dhruv Srivastav",
  "license": "UNLICENSED",
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "scripts": {
    "commit": "git add . && git cz",
    "release:patch": "standard-version --patch && git push --follow-tags",
    "release:minor": "standard-version --minor && git push --follow-tags",
    "lint:all": "eslint . --config .eslintrc.json --ext .ts,.tsx",
    "lint-fix:all": "eslint . --config .eslintrc.json --ext .ts,.tsx --fix",
    "pretty:all": "prettier --write \"src/**/*.(ts|tsx)\"",
    "clean": "rm -r dist || true",
    "start": "webpack serve --config webpack.config.js --env local --hot --host 0.0.0.0",
    "build:dev": "yarn clean && webpack --config webpack.config.js --env dev --mode production && (rm -rf ./dist/**/*.js.map || true)",
    "build:prod": "yarn clean && webpack --config webpack.config.js --env prod --mode production && (rm -rf ./dist/**/*.js.map || true)",
    "start:server": "nodemon --exec ts-node server/index.ts",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@babel/polyfill": "^7.11.5",
    "apollo-server-express": "^2.9.9",
    "axios": "^1.3.4",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "process": "^0.11.10",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/client-preset": "^2.1.0",
    "@mdx-js/react": "^1.6.22",
    "@types/express": "^4.17.17",
    "@types/mz": "^2.7.1",
    "@types/node": "^18.14.1",
    "@types/promise-polyfill": "^6.0.3",
    "@types/react": "latest",
    "@types/react-dom": "17.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "packageManager": "yarn@3.2.2"
}
